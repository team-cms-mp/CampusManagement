@model IEnumerable<CampusManagement.Models.SF_GetApplicantStudentChallansFeeDefaulterSummary_Result>

@{
    ViewBag.Title = "Challans Fee Defaulter";
    ////Layout = "~/Views/Shared/_MyLayout.cshtml";
}

<div class="row">
    <h2><span class="col-sm-12">@ViewBag.Title</span></h2>
</div>
<hr style="border-top: 1px solid #6d9b90;" />

<table class="table">
    <tr>
        <th>
            Form No.
        </th>
    
       
        <th>
            Challan No.
        </th>
        
        <th>
            Payable
        </th>
        <th>
            Issue Date
        </th>
        <th>
            Last Date
        </th>
        
        <th>
            Is Deposited
        </th>
        <th>
            Payment Mode
        </th>
       
       
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.FormNo)
            </td>
            
            <td>
                @Html.DisplayFor(modelItem => item.ChallanID)
            </td>
          
            <td>
                @Html.DisplayFor(modelItem => item.Amount)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IssueDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LastDate)
            </td>
           
            
            <td>
                @Html.DisplayFor(modelItem => item.IsDeposited)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Account_No)
            </td>
           
           
        </tr>
    }

</table>


