@model CampusManagement.Models.SP_ProgramForPage_Result
<h4 class="alert alert-info">Are you sure you want to delete this?</h4>

<div>
    <dl class="dl-horizontal">
        <dt>
            <label class="control-label">Program ID</label>
        </dt>

        <dd>
            @Html.DisplayFor(model => model.ProgramID)
        </dd>

        <dt>
            <label class="control-label">Program Name</label>
        </dt>

        <dd>
            @Html.DisplayFor(model => model.ProgramName)
        </dd>

        <dt>
            <label class="control-label">Program Code</label>
        </dt>

        <dd>
            @Html.DisplayFor(model => model.ProgramCode)
        </dd>

        <dt>
            <label class="control-label">Program Type</label>
        </dt>

        <dd>
            @Html.DisplayFor(model => model.ProgramTypeName)
        </dd>

        <dt>
            <label class="control-label">Faculty / Departments</label>
        </dt>

        <dd>
            @Html.DisplayFor(model => model.FacultyName)
        </dd>

        <dt>
            <label class="control-label">Level</label>
        </dt>

        <dd>
            @Html.DisplayFor(model => model.LevelName)
        </dd>

        <dt>
            <label class="control-label">Entry Test Total Marks</label>
        </dt>

        <dd>
            @Html.DisplayFor(model => model.TestTotalMarks)
        </dd>

        <dt>
            <label class="control-label">Drawing Test Total Marks</label>
        </dt>

        <dd>
            @Html.DisplayFor(model => model.DrawingTestTotalMarks)
        </dd>

        <dt>
            <label class="control-label">Interview Total Marks</label>
        </dt>

        <dd>
            @Html.DisplayFor(model => model.InterviewTotalMarks)
        </dd>

        <dt>
            <label class="control-label">Description</label>
        </dt>

        <dd>
            @Html.DisplayFor(model => model.ProgramDescription)
        </dd>

        <dt>
            <label class="control-label">Is Active</label>
        </dt>

        <dd>
            @Html.DisplayFor(model => model.IsActive)
        </dd>

        <dt>
            <label class="control-label">Created On</label>
        </dt>

        <dd>
            @Html.DisplayFor(model => model.CreatedOn)
        </dd>

        <dt>
            <label class="control-label">Created By</label>
        </dt>

        <dd>
            @Html.DisplayFor(model => model.CB_Username)
        </dd>

        <dt>
            <label class="control-label">Modified On</label>
        </dt>

        <dd>
            @*@Html.DisplayFor(model => model.ModifiedOn)*@
        </dd>
        <dt>
            <label class="control-label">Modified By)</label>
        </dt>

        <dd>
            @Html.DisplayFor(model => model.MB_Username)
        </dd>

    </dl>

    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Delete" class="btn btn-primary" />
            <a href="@Url.Content("~/Programs/Index")" class="btn btn-primary">Cancel</a>
        </div>
    }
</div>
