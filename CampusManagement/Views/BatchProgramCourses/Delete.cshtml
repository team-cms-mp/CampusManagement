@model CampusManagement.Models.GetBatchProgramCourses_Result
@using CampusManagement.Models
<h4 class="alert alert-info">Are you sure you want to delete this?</h4>

<div>
    <dl class="dl-horizontal">
        <dt>
            <label class="control-label">Session</label>
        </dt>

        <dd>
            @Html.DisplayFor(model => model.BatchName)
        </dd>

        <dt>
            <label class="control-label">Program</label>
        </dt>

        <dd>
            @Html.DisplayFor(model => model.ProgramName)
        </dd>

        <dt>
            <label class="control-label">Subject</label>
        </dt>

        <dd>
            @Html.DisplayFor(model => model.CourseName)
        </dd>

        <dt>
            <label class="control-label">Subject Type</label>
        </dt>

        <dd>
            @Html.DisplayFor(model => model.CourseTypeName)
        </dd>
        <dt>
            <label class="control-label">Section</label>
        </dt>

        <dd>
            @Html.DisplayFor(model => model.SectionName)
        </dd>
        <dt>
            <label class="control-label">Lecture Room</label>
        </dt>

        <dd>
            @Html.DisplayFor(model => model.LectureRoomName)
        </dd>
        <dt>
            <label class="control-label">Per Week Lecure Count</label>
        </dt>

        <dd>
            @Html.DisplayFor(model => model.PerWeekLectureCount)
        </dd>

        <dt>
            <label class="control-label">Per Week Lab Count</label>
        </dt>

        <dd>
            @Html.DisplayFor(model =>model.PerWeekLabCount)
        </dd>

        <dt>
            <label class="control-label">@Html.DisplayNameFor(model => model.YearSemesterNo)</label>
        </dt>

        <dd>
            @Html.DisplayFor(model => model.YearSemesterNo)
        </dd>

        <dt>
            <label class="control-label">Is Active</label>
        </dt>

        <dd>
            @Html.DisplayFor(model => model.IsActive)
        </dd>

        <dt>
            <label class="control-label">Created On</label>
        </dt>

        <dd>
            @Html.DisplayFor(model => model.CreatedOn)
        </dd>

        <dt>
            <label class="control-label">Created By</label>
        </dt>

        <dd>
            @Html.DisplayFor(model => model.CreatedBy)
        </dd>

        <dt>
            <label class="control-label">Modified On</label>
        </dt>

        <dd>
            @Html.DisplayFor(model => model.ModifiedOn)
        </dd>
        <dt>
            <label class="control-label">Modified By</label>
        </dt>

        <dd>
            @Html.DisplayFor(model => model.ModifiedBy)
        </dd>

    </dl>

    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Delete" class="btn btn-primary" />
            <a href="@Url.Content("~/BatchProgramCourses/Index")" class="btn btn-primary">Cancel</a>
        </div>
    }
</div>
