@model CampusManagement.Models.GetStudentDiscountRequests_Result

<h4 class="alert alert-info">Are you sure you want to delete this?</h4>


<div>
    <dl class="dl-horizontal">
        <dt>
            <label class="control-label">Request</label>
        </dt>

        <dd>
            @Html.DisplayFor(model => model.RequestDescription)
        </dd>

        <dt>
            <label class="control-label">Request Status</label>
        </dt>

        <dd>
            @Html.DisplayFor(model => model.StudentRequestStatusName)
        </dd>

        <dt>
            <label class="control-label">Discount Percentage</label>
        </dt>

        <dd>
            @Html.DisplayFor(model => model.StudentRequestTypeID)
        </dd>

    </dl>

    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Delete" class="btn btn-primary" />
            <a href="@Url.Content("~/StudentDiscountRequests/Index")" class="btn btn-primary">Cancel</a>
        </div>
    }
</div>
